<Application x:Class="WPF_MVVM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPF_MVVM"
             xmlns:vm ="clr-namespace:WPF_MVVM.ViewModels"
             xmlns:vs ="clr-namespace:WPF_MVVM.Views">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:VehiclesViewModel}">
                <vs:VehiclesView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:VehicleViewModel}">
                <vs:VehicleView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
